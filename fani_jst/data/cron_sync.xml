<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sync JST Shops daily -->
        <record id="ir_cron_sync_jst_shop" model="ir.cron">
            <field name="name">Sync JST Shops Daily</field>
            <field name="model_id" ref="fani_jst.model_jst_shop"/>
            <field name="state">code</field>
            <field name="code">model._action_sync_jst_shop_daily()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <!-- Sync JST products daily -->
        <record id="ir_cron_sync_jst_products" model="ir.cron">
            <field name="name">Sync JST Products Daily</field>
            <field name="model_id" ref="fani_jst.model_jst_product_template"/>
            <field name="state">code</field>
            <field name="code">model._action_sync_jst_products_daily()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <!-- Sync JST sale Orders: 5p -->
        <record id="ir_cron_sync_jst_orders" model="ir.cron">
            <field name="name">Sync JST Orders</field>
            <field name="model_id" ref="fani_jst.model_jst_sale_order"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_jst_orders()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
  
        <!-- Sync JST After-sale Orders: 1 days -->
        <record id="ir_cron_sync_jst_after_orders" model="ir.cron">
            <field name="name">Sync JST After-Sale Orders</field>
            <field name="model_id" ref="fani_jst.model_jst_sale_order_after"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_jst_after_orders()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <record id="ir_cron_sync_jst_orders_past" model="ir.cron">
            <field name="name">Sync JST Orders (Past)</field>
            <field name="model_id" ref="fani_jst.model_jst_sale_order"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_jst_orders_past(duration=1)
            </field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <record id="ir_cron_sync_jst_warehouse" model="ir.cron">
            <field name="name">Sync JST Warehouses</field>
            <field name="model_id" ref="fani_jst.model_jst_warehouse"/>
            <field name="state">code</field>
            <field name="code">model._action_sync_jst_warehouses_weekly()
            </field>
            <field name="interval_number">1</field>
            <field name="interval_type">weeks</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <record id="ir_cron_sync_jst_product_inventory" model="ir.cron">
            <field name="name">Sync JST Product Inventory</field>
            <field name="model_id" ref="fani_jst.model_jst_product_inventory"/>
            <field name="state">code</field>
            <field name="code">model._sync_jst_product_inventory()
            </field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
        <record id="ir_cron_sync_jst_warehouse_inventory" model="ir.cron">
            <field name="name">Sync JST Warehouse Inventory</field>
            <field name="model_id" ref="fani_jst.model_jst_warehouse_inventory"/>
            <field name="state">code</field>
            <field name="code">model._sync_jst_warehouse_inventory()
            </field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
 
        <record id="ir_cron_sync_jst_other_inout" model="ir.cron">
            <field name="name">Sync JST Other Delivery Orders</field>
            <field name="model_id" ref="fani_jst.model_jst_stock_inout"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_other_inouts(duration=24)
            </field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <record id="ir_cron_sync_jst_inout" model="ir.cron">
            <field name="name">Sync JST Delivery Orders</field>
            <field name="model_id" ref="fani_jst.model_jst_stock_inout"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_inouts(duration_minutes=10)
            </field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
        <record id="ir_cron_sync_jst_inout_past" model="ir.cron">
            <field name="name">Sync JST Delivery Orders (Past)</field>
            <field name="model_id" ref="fani_jst.model_jst_stock_inout"/>
            <field name="state">code</field>
            <field name="code">model._cron_sync_inouts_past(duration_hours=2)
            </field>
            <field name="interval_number">6</field>
            <field name="interval_type">minutes</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
    </data>
</odoo> 