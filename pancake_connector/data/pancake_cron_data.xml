<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Pancake Sync Cron Job -->
    <record id="ir_cron_pancake_sync" model="ir.cron">
        <field name="name">Pancake Data Sync</field>
        <field name="model_id" ref="model_pancake_shop"/>
        <field name="state">code</field>
        <field name="code">model._cron_sync_pancake_data()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="user_id" ref="base.user_root"/>
    </record>
</odoo>
