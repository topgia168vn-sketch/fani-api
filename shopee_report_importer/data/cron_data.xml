<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Import Downloaded Data Cron -->
    <record id="cron_import_downloaded_data" model="ir.cron">
        <field name="name">Import Downloaded Shopee Data</field>
        <field name="model_id" ref="model_shopee_report_mixin"/>
        <field name="state">code</field>
        <field name="code">model._cron_import_downloaded_data()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="active">True</field>
        <field name="user_id" ref="base.user_root"/>
    </record>
</odoo>
