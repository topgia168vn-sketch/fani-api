        <!-- Cron job to refresh YonSuite access token -->
        <record id="ir_cron_yonsuite_refresh_token" model="ir.cron">
            <field name="name">YonSuite: Refresh Access Token</field>
            <field name="model_id" ref="model_res_config_settings" />
            <field name="state">code</field>
            <field name="code">model.get_access_token_if_needed()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
        </record>

